{
  "manifest_version": 3,
  "name": "Dark Pattern Detection on X",
  "version": "0.1.0",
  "description": "To detect dark patterns on X.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "Click to show an alert",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://x.com/*"],
      "js": ["content-scripts/emotional-steering.js"]

    }
  ]
}
